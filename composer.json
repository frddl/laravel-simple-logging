{
    "name": "frddl/laravel-simple-logging",
    "description": "A simple, elegant logging package for Laravel with automatic method wrapping and beautiful web interface",
    "version": "1.1.2",
    "keywords": ["laravel", "logging", "debugging", "monitoring", "tracing"],
    "license": "MIT",
    "authors": [
        {
            "name": "Farid Mammadov (@frddl)",
            "email": "faridmmmdv@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0|^11.0",
        "illuminate/database": "^9.0|^10.0|^11.0",
        "illuminate/http": "^9.0|^10.0|^11.0",
        "illuminate/view": "^9.0|^10.0|^11.0",
        "illuminate/routing": "^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Frddl\\LaravelSimpleLogging\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Frddl\\LaravelSimpleLogging\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Frddl\\LaravelSimpleLogging\\SimpleLoggingServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
